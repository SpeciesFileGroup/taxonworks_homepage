<div class="getting-started-section">
    <img class="getting-started-section__leaves" src="images/leaves-bottom.svg">
    <article class="getting-started-section__section">
        <div class="getting-started-section__container-1">
            <div class="getting-started-section__section-text">
                <h2 class="getting-started-section__title">{{ gettingStartedTitle }}</h2>
                <img class="getting-started-section__logo-image" src="{{ gettingStartedLogo }}" alt="This is the logo of {{ title }}">
            </div>
            <div class="getting-started-section__copy">
                {{ gettingStarted1HTML | safe }}
            </div>
        </div>

        <div class="getting-started-section__container-2">
            <div class="getting-started-section__copy getting-started-section__copy--with-gutter">
                {{ gettingStarted2aHTML | safe }}
            </div>
            <div class="getting-started-section__copy">
                {{ gettingStarted2bHTML | safe }}
            </div>
        </div>
    </article>

</div>
