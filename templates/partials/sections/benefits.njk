<div class="benefits-section">
    <article class="benefits-section__section-1">
        <div class="benefits-section__container wrapper">
            <h2 class="benefits-section__title item">{{ benefitsTitle  }}</h2>
            <div class="benefits-section__copy item">
                {{ benefits1HTML | safe }}
            </div>
            <div class="benefits-section__mobile-image item">
                <img src="images/inhs-01.svg" alt="INHS 01" />
            </div>
        </div>
        <div class="benefits-section__container wrapper">
            <div class="benefits-section__container__illustrations wrapper">
                <div class="benefits-section__illustration__1">
                    <img src="images/inhs-01.svg" alt="" />
                </div>
                <div class="benefits-section__illustration__2">
                    <img src="images/inhs-02.svg" alt="" />
                </div>
                <div class="benefits-section__illustration__3">
                    <img src="images/inhs-03.svg" alt="" />
                </div>
                <div class="benefits-section__illustration__4">
                    <img src="images/inhs-04.svg" alt="" />
                </div>
            </div>
        </div>
    </article>

    <article class="benefits-section__section-2">
        <div class="benefits-section__container wrapper">
            <div class="benefits-section__copy item">
                {{ benefits2HTML | safe }}
            </div>
            <div class="benefits-section__illustration item">
                <img src="images/inhs-06.svg" class="benefits-section__illustration__large" alt="INHS 06" />
                <img src="images/inhs-05.svg" class="benefits-section__illustration__large" alt="INHS 05" />
                <img src="images/inhs-05.svg" class="benefits-section__illustration__small" alt="INHS 05" />
            </div>
        </div>
    </article>

    <article class="benefits-section__section-3">
        <div class="benefits-section__container wrapper">
            <div class="benefits-section__empty item"></div>
            <div class="benefits-section__copy item">
                {{ benefits3HTML | safe }}
            </div>
            <div class="benefits-section__mobile-image item">
                <img src="images/branches-2-small.svg" width="272px" height="497px" alt="Branches 2" />
            </div>
        </div>
    </article>

    <article class="benefits-section__section-4">
        <div class="benefits-section__container wrapper">
            <div class="benefits-section__container__inner wrapper item">
                <div class="benefits-section__copy item">
                    {{ benefits4HTML | safe }}
                </div>
                <div class="benefits-section__sponsor-grid item">
                    <div class="benefits-section__sponsor-grid__grid wrapper">
                        {% for sponsor in sponsors %}
                            <img class="benefits-section__sponsor-grid__sponsor item" src="{{ sponsor.src }}" alt="{{ sponsor.name }}" />
                        {% endfor %}
                    </div>
                </div>
            </div>
            <div class="benefits-section__empty item"></div>
            <div class="benefits-section__mobile-image item">
                <img src="images/trunk-small.svg" width="320px" height="636px" alt="Trunk" />
            </div>
        </div>
    </article>
</div>